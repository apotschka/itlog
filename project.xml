<project
    name = "itlog"
    description = "A portable logger for monitoring iterative numerical algorithms"
    script = "zproject.gsl"
    license = "MPLv2"
    prefix = "il"
    linkname = "itlog"
    email = "potschka@iwr.uni-heidelberg.de"
    url = "https://github.com/apotschka/itlog"
    repository = "https://github.com/apotschka/itlog"
    >
    <version major = "0" minor = "4" />
    <include filename = "license.xml" />
    <use project = "czmq" />

    <target name = "debian" />
    <target name = "travis" />
    <target name = "vs2015" />
    <target name = "python" />

    <class name = "il_column" private = "1" state = "stable">Column data container.</class>
    <class name = "il_fid_list" private = "1" />

    <class name = "il_log" />

</project>
